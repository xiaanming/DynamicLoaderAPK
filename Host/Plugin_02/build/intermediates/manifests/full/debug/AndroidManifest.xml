<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pluginactivity"
    android:versionCode="1"
    android:versionName="1.00x6b" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="23" />

    <application
        android:name="com.example.pluginactivity.PluginApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Holo.Light.DarkActionBar" >
        <activity
            android:name="com.example.pluginactivity.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.pluginactivity.PluginActionBarActivity"
            android:theme="@style/Theme.AppCompat.Light" />
        <activity
            android:name="com.example.pluginactivity.NotRegisterActivity"
            android:theme="@style/Theme.AppCompat.Light" >
        </activity>
    </application>

</manifest>